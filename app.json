{
  "expo": {
    "name": "SOLENT INSPIRE",
    "slug": "creativeapplication",
    "privacy": "public",
    "version": "1.0.0",
    "orientation": "portrait",
    "githubUrl": "https://github.com/brucebonus/creative-application",
    "icon": "./assets/fire.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": "A Creative Collaboration Application - Browse for inspiration and upload your creativity"
  }
}
